<h1> Caregivers </h1>

<table>
  <thead>
    <tr>
      <th>
        First Name
      </th>
      <th>
        Last Name
      </th>
      <th>
        Phone Number
      </th>
    </tr>
<% @caregivers.each do |caregiver| %>
  <tr>
    <td>
      <%= caregiver.first_name %>
    </td>
    <td>
      <%= caregiver.last_name %>
    </td>
    <td>
      <%= caregiver.phone_number %>
    </td>
  </tr>
<% end %>
</table>
